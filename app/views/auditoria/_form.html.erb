<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%= form_for @auditorium do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :capacity  %>
  <%= f.text_field :capacity %>
  <br>
  <%= f.submit %>
<% end %>