<% if current_user %>
  <%= link_to 'Add Movie', new_movie_path %>
<% end %>

<% @movies.each do |movie| %>
  <%= link_to '<%= movie.title %>', movie %>

  <% if current_user %>
    <%= link_to 'Edit', edit_movie_path(post) %>
    <%= link_to 'Destroy', movie, method: :delete, data: {confirm: 'Are you sure you want to delete this movie?'} %>
  <% end %>
<% end %>